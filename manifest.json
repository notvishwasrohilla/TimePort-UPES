{
  "manifest_version": 3,
  "name": "Timetable Scraper",
  "version": "1.0",
  "description": "Scrapes the daily timetable from the student portal and displays it.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
